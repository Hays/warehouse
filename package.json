{
  "name": "warehouse",
  "version": "0.0.1",
  "description": "simple warehouse system",
  "main": "index.js",
  "scripts": {
    "cleanBuild": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files --source-maps",
    "build": "babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files --source-maps",
    "release": "babel ./ --out-dir release/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files --no-comments --compact true --minified",
    "start": "npm run build && node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hays/warehouse.git"
  },
  "keywords": [
    "warehouse"
  ],
  "author": "Hays",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Hays/warehouse/issues"
  },
  "homepage": "https://github.com/Hays/warehouse#readme",
  "dependencies": {
    "koa": "^2.5.0",
    "koa-body": "^2.5.0",
    "koa-compose": "^4.0.0",
    "koa-router": "^7.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.7.0",
    "rimraf": "^2.6.2"
  }
}
